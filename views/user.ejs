<%- include('includes/head.ejs') %>
    <link rel="stylesheet" href="/css/user-list.css">
</head>
<body>
    <%- include('includes/navbar.ejs') %>

    <main>
        <% if(usrs.length > 0) {%>
            <h1>Users</h1>
            <h3>
            <% for(const user of usrs){ %>
            <ul>
                <li class="main_li"><%= user.username %></li>
            </ul>
            <% } %>
            </h3>
        <% }else{ %>
            <h1>No User Found. Please Add User To See On The Page.</h1>
        <% } %>
    </main>
</body>
</html>